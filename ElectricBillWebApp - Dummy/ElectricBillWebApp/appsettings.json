{
  "ConnectionStrings": {
    "Default": "Server=(LocalDB)\\localdb;Database=EBSDB;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    },
    "CustomLogger": {
      "Options": {
        "LogFolderPath": "Log",
        "LogFileName": "EBS_LOG.log",
        "LogLevel": "All",
        "MaximumFileSize": 314572800, // 300 MB
        "MaxSizeRollBackups": 3
      }
    }
  },
  "AllowedHosts": "*"
}
